    <script>

        // Tab deÄŸiÅŸtirme fonksiyonu
        function showTab(tabName) {
            // TÃ¼m tab iÃ§eriklerini gizle
            const contents = document.querySelectorAll('.tab-content');
            contents.forEach(content => content.classList.remove('active'));
            
            // TÃ¼m tab butonlarÄ±ndan active class'Ä± kaldÄ±r
            const tabs = document.querySelectorAll('.tab');
            tabs.forEach(tab => tab.classList.remove('active'));
            
            // SeÃ§ili tabÄ± gÃ¶ster
            const selectedTab = document.getElementById(tabName);
            if (selectedTab) {
                selectedTab.classList.add('active');
            }
            
            // TÄ±klanan butonu active yap
            event.target.classList.add('active');
        }

        // Test seÃ§imi iÃ§in JavaScript
        function selectAnswer(element, questionDiv) {
            const allOptions = questionDiv.querySelectorAll('.test-options li');
            const correctAnswer = questionDiv.getAttribute('data-correct');
            
            // Daha Ã¶nce seÃ§ilmiÅŸleri temizle
            allOptions.forEach(opt => {
                opt.classList.remove('selected', 'wrong', 'correct-answer');
            });
            
            // SeÃ§ilen ÅŸÄ±kkÄ±n harfini al (A, B, C, D)
            const selectedLetter = element.textContent.trim()[0];
            
            // SeÃ§ilen ÅŸÄ±kkÄ± iÅŸaretle
            if (selectedLetter === correctAnswer) {
                element.classList.add('correct-answer');
            } else {
                element.classList.add('wrong');
                // DoÄŸru cevabÄ± da gÃ¶ster
                allOptions.forEach(opt => {
                    if (opt.textContent.trim()[0] === correctAnswer) {
                        opt.classList.add('correct-answer');
                    }
                });
            }
        }

        // Sayfa yÃ¼klendiÄŸinde tÃ¼m test sorularÄ±na event listener ekle
        document.addEventListener('DOMContentLoaded', function() {
            const testQuestions = document.querySelectorAll('.test-question');
            
            testQuestions.forEach(question => {
                const options = question.querySelectorAll('.test-options li');
                options.forEach(option => {
                    option.addEventListener('click', function() {
                        selectAnswer(this, question);
                    });
                });
            });
        });

    </script>
